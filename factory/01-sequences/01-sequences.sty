% == PACKAGES USED == %

\RequirePackage{mathtools}


% == DEFINITIONS == %

% #1 .. #5 : here are the arguments will be printed.
%
%    #5    #4
%       #1
%    #2    #3

\newcommand\seqsuprageo[5]{%
	\prescript{#5}{#2}{#1}_{#3}^{#4}%
}


\newcommand\seqhypergeo[3]{%
    \seqsuprageo{#1}{#2}{#3}{}{}%
}


\newcommand\seqplus[3]{%
    \seqsuprageo{#1}{}{#2}{#3}{}%
}
